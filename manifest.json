{
    "manifest_version": 2,
    "name": "Registration Helper for UF",
    "version": "0.1",
    "options_page": "options.html",
    "content_scripts": [
        {
          "matches": [
            "https://one.uf.edu/soc/registration-search/*", "https://one.uf.edu/soc/*"
          ],
          "js": ["jquery.min.js", "content.js"]
          
        }        
      ],
      "browser_action": {
        "default_icon": "icons/icon16.png"
      },
      "icons": { "16": "icons/icon16.png",
        "48": "icons/icon48.png",
       "128": "icons/icon128.png" },
      
      "permissions": [
        "activeTab",
        "storage"
      ]
  }